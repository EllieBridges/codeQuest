import React from "react";
import { Link } from "react-router-dom";


const PlayButton = (onClick, text, enabled, route) => {
    return (
        <Link to={route}>
            <button onClick={`() => ${onClick}${(text)}`} disabled={!enabled} className={`${text} button`}>{text}</button>
        </Link>
    )
}

export default PlayButton;